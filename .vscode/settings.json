{
  "editor.formatOnSave": true,
  "eslint.validate": [
    "vue",
    "yaml",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.words": ["Bilibili", "esno", "Juejin", "Nuxt", "ofetch", "ohmyfetch", "pnpm"],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/**",
    ".vscode/**",
    ".vscode-insiders/**",
    "./output/**"
  ]
}
